<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.box.server</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/box</string>
        <string>--server</string>
    </array>

    <key>UserName</key>
    <string>thierry</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>HOME</key>
        <string>/Users/thierry</string>
    </dict>

    <key>WorkingDirectory</key>
    <string>/Users/thierry</string>

    <key>StandardOutPath</key>
    <string>/Users/thierry/Library/Logs/box-server.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/thierry/Library/Logs/box-server.log</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
</dict>
</plist>
